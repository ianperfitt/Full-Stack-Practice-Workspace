# Database connection settings
hibernate.connection.url=jdbc:postgresql://localhost:5432/SpringDevDb
hibernate.connection.username=postgres
hibernate.connection.password=postgres

# Echo all executed SQL to console
hibernate.show_sql=true
hibernate.format_sql=true
hibernate.highlight_sql=true

# Automatically export the schema
hibernate.hbm2ddl.auto=create

jakarta.persistence.jdbc.url=jdbc:postgresql://localhost:5432/SpringDevDb
jakarta.persistence.jdbc.user=postgres
jakarta.persistence.jdbc.password=postgres